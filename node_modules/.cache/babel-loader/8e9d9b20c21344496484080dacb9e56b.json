rent();\r\n      if(checkUseer){\r\n        formState.signup=false\r\n        formState.signin=false\r\n      }\r\n      else{\r\n        formState.logout=false\r\n      }\r\n\r\n    });\r\n\r\n    return { formState, Signup,Signin,Logout };\r\n  }\r\n});\r\n</script>\r\n    "]},"metadata":{},"sourceType":"module"}