{"ast":null,"code":"import { defineComponent, reactive, onMounted } from \"vue\"; // import {Loader} from 'google-maps';\n\nimport { Loader } from \"@googlemaps/js-api-loader\"; // import Parse from 'parse'\n// const mapOptions = {\n//   center: {\n//     lat: 0,\n//     lng: 0\n//   },\n//   zoom: 4\n// };\n\nexport default defineComponent({\n  component: {},\n\n  setup() {\n    const formState = reactive({\n      image: \"\",\n      center: null,\n      loader: false\n    });\n    onMounted(async () => {\n      // or const {Loader} = require('google-maps'); without typescript\n      const loader = new Loader({\n        apiKey: \"AIzaSyABPywZVGnAsgP8llgiBFnx8sAvUUiRyv4\"\n      });\n      await loader.load().then(async google => {\n        if (navigator.geolocation) {\n          navigator.geolocation.getCurrentPosition(async function (position) {\n            console.log(\"position\", position);\n            var point = await new google.maps.LatLng(position.coords.latitude, position.coords.longitude); // Initialize the Google Maps API v3\n\n            var map = await new google.maps.Map(document.getElementById('map'), {\n              zoom: 15,\n              center: point,\n              mapTypeId: google.maps.MapTypeId.ROADMAP\n            }); // Place a marker\n\n            await new google.maps.Marker({\n              position: point,\n              map: map\n            });\n          });\n        }\n      }).catch(e => {\n        // do something\n        console.log(e);\n      });\n    });\n    return {\n      formState\n    };\n  }\n\n});","map":{"version":3,"mappings":"AAYA,SAASA,eAAT,EAA0BC,QAA1B,EAAoCC,SAApC,QAAqD,KAArD,C,CACA;;AACA,SAASC,MAAT,QAAuB,2BAAvB,C,CAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA,eAAeH,eAAe,CAAC;EAC7BI,SAAS,EAAE,EADkB;;EAE7BC,KAAK,GAAG;IACN,MAAMC,SAAQ,GAAIL,QAAQ,CAAC;MACzBM,KAAK,EAAE,EADkB;MAEzBC,MAAM,EAAE,IAFiB;MAGzBC,MAAM,EAAC;IAHkB,CAAD,CAA1B;IAKAP,SAAS,CAAC,YAAY;MACpB;MAEA,MAAMO,MAAK,GAAI,IAAIN,MAAJ,CAAW;QACxBO,MAAM,EAAE;MADg